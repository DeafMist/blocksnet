{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "blocks_cutter",
    "type": "object",
    "title": "blocks_cutter",
    "description": "geojson FeatureCollection of polygons or multipolygons without any properties",
    "required": ["type", "features"],
    "properties": {
      "type": {
        "type": "string",
        "enum": ["FeatureCollection"]
      },
      "features": {
        "type": "array",
        "items": {
        "type": "object",
        "required": ["type", "geometry"],
        "properties": {
          "type": {

          },
          "geometry": {
            "anyOf": [
              { "$ref": "polygon" },
              { "$ref": "multi_polygon" }
            ]
          }
        }
      }
      }
    }
}